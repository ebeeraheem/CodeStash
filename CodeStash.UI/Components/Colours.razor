@page "/colours"

<MudGrid Spacing="6" Justify="Justify.Center">
    <MudItem>
        <MudPaper Height="140px" Width="140px" Style="@($"background:{CodeStashColors.Primary}")"/>
    </MudItem>
    <MudItem>
        <MudPaper Height="140px" Width="140px" Style="@($"background:{CodeStashColors.Secondary}")" />
    </MudItem>
    <MudItem>
        <MudPaper Height="140px" Width="140px" Style="@($"background:{CodeStashColors.Tertiary}")" />
    </MudItem>
    <MudItem>
        <MudPaper Outlined="true" Height="140px" Width="140px" Style="@($"background:{CodeStashColors.Background}")" />
    </MudItem>
</MudGrid>
